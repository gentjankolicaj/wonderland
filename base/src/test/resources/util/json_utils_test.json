{
  "gmail": {
    "host": "${GMAIL_HOST:smtp.gmail.com}",
    "port": 587,
    "username": "${GMAIL_USERNAME:test@gmail.com}",
    "password": "${GMAIL_PASSWORD:101}",
    "properties": {
      "mail": {
        "transport.protocol": "smtp",
        "smtp": {
          "auth": true,
          "connectionTimeout": 5000,
          "timeout": 5000,
          "writeTimeout": 5000,
          "starttls.enable": true
        }
      }
    }
  }
}